{
  "name": "lineup-fullstack",
  "version": "1.0.0",
  "description": "Sistema de filas médicas LineUp - Gestion complète de files d'attente médicales",
  "main": "server/index.js",
  "scripts": {
    "start": "cd server && npm start",
    "dev": "cd server && npm run dev",
    "build": "npm run install-all && cd client && npm run build",
    "build:server": "cd server && npm install",
    "build:client": "cd client && npm install && npm run build",
    "install-all": "npm run install-server && npm run install-client",
    "install-server": "cd server && npm install",
    "install-client": "cd client && npm install",
    "test": "cd server && npm test",
    "lint": "cd server && npm run lint",
    "clean": "rm -rf node_modules client/node_modules server/node_modules client/dist"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "medical",
    "queue",
    "healthcare",
    "react",
    "nodejs",
    "mongodb"
  ],
  "author": "LineUp Medical System",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/lineup-fullstack.git"
  },
  "dependencies": {
    "csurf": "^1.11.0",
    "rate-limit-redis": "^4.2.2",
    "redis": "^5.8.2"
  }
}
